
@{
    ViewBag.Title = "會員登入";
}

<h2>會員登入</h2>

<form action="@Url.Action("Signin")" method="post">
    <hr />
    <div class=" form-group">
        <span class=" control-label col-md-2">帳號</span>
        <div class="col-md-10">
            <input type="text" id="Mem_id" name="Mem_id" required="required" class="form-control" placeholder="這是placeholder的效果" />
        </div>
    </div>


    <div class=" form-group">
        <span class=" control-label col-md-2">密碼</span>
        <div class="col-md-10">
            <input type="password" id="Mem_password" name="Mem_password" required="required" class="form-control" />
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="登入" class="btn btn-default" />
            @ViewBag.Message
        </div>
    </div>

    <p>
        <input type="checkbox" name="checkbox1" checked="checked" value="測試" />
        測試
    </p>
    <p>
        測試
        <input type="date" name="checkbox1" class="form-control"/>
    </p>
</form>


